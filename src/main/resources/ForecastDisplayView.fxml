      </Label>
      <Label fx:id="tempForecast" layoutX="100.0" layoutY="60.0" prefHeight="27.0" prefWidth="254.0" text="temperature: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="pressureForecast" layoutX="100.0" layoutY="84.0" prefHeight="27.0" prefWidth="254.0" text="pressure: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="humidityForecast" layoutX="100.0" layoutY="108.0" prefHeight="27.0" prefWidth="254.0" text="humidity: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="imageWeather" fitHeight="76.0" fitWidth="83.0" layoutX="8.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
