<?import javafx.scene.text.Font?>

<AnchorPane fx:id="forecastView" prefHeight="103.0" prefWidth="278.0" style="-fx-background-color: #80ff80;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
